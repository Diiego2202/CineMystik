<h1>
	Lista de filmes cadastradas
</h1>

<% for (let i = 0; i < filme.length; i++) { %>
<div>

	<h2>
		<%= filme[i].nome %>
	</h2>

	<p>
		Ano: <%= filme[i].ano %>
	</p>

	<p>
		Diretor: <%= filme[i].diretor %>
	</p>

    <p>
		genero: <%= filme[i].genero %>
	</p>

    <p>
		subgenero: <%= filme[i].subgenero %>
	</p>

    <p>
		sinopse: <%= filme[i].sinopse %>
	</p>

	<hr />

</div>
<% } %>
